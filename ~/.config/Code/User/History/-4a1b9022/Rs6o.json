{
    "files.associations": {
        ".env*": "dotenv"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "keyword.other.dotenv",
                "settings": {
                    "foreground": "#FF000000"
                }
            }
        ]
    },
    "workbench.preferredHighContrastColorTheme": "GitHub Dark Default",
    "workbench.iconTheme": "vscode-icons",
    "editor.fontSize": 20,
    "files.autoSave": "afterDelay"
}